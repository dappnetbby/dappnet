{
  "manifest_version": 3,
  "name": "Dappnet",
  "version": "0.2.0",
  "description": "Access .eth dapps that can't be taken down.",
  "homepage_url": "https://freedom.eth",
  "icons": {
    "128": "../../icon-128.png"
  },
  "background": {
    "service_worker": "index.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "128": "../../icon-128.png"
    },
    "default_title": "dappnet",
    "default_popup": "popup.html"
  },
  "permissions": [
    "proxy",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
